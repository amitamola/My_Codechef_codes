
import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner s=new Scanner(System.in);
		int i;
		int a=s.nextInt();
		
		for(i=1;i<=a;i++)
		{
		    long w=1,zeros=0;
		    long b=s.nextLong();
		    while((b/Math.pow(5,w))>0)
		    {
		        zeros=zeros+(long)(b/Math.pow(5,w));
		        w++;
		    }
		    
		    System.out.println(zeros);
		}		
	}
	    
 }
